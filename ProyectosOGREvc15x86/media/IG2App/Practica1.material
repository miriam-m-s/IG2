material IG2/Space{
		technique
					{
						pass
								{
								lighting off
								diffuse 1.0 1.0 1.0					
								ambient 0.5 0.5 0.5
								texture_unit
								{ 
									tex_coord_set 0 
									texture spaceSky.jpg
									colour_op modulate
									rotate_anim 0.05
								}

								texture_unit
								{ 
									tex_coord_set 0 
									texture lightMap.jpg
									colour_op modulate
								}
								
						}
					}
}

