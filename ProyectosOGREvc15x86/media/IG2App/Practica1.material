material Practica1/Smoke
{
	technique
			{
				pass
				{
					lighting off
					scene_blend alpha_blend
					depth_write off
					diffuse vertexcolour
					texture_unit
					{
						texture smoke.png
						tex_address_mode clamp
					}
				}
			}
}

